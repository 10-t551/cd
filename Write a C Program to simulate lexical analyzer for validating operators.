#include <stdio.h>
#include <string.h>
void identify_operator(char *op) {
 if (strcmp(op, "+") == 0)
 printf("Addition Operator\n");
 else if (strcmp(op, "-") == 0)
 printf("Subtraction Operator\n");
 else if (strcmp(op, "*") == 0)
 printf("Multiplication Operator\n");
 else if (strcmp(op, "/") == 0)
 printf("Division Operator\n");
 else if (strcmp(op, "%") == 0)
 printf("Modulus Operator\n");
 else if (strcmp(op, "=") == 0)
 printf("Assignment Operator\n");
 else if (strcmp(op, "==") == 0)
 printf("Equal to Operator\n");
 else if (strcmp(op, "!=") == 0)
 printf("Not Equal to Operator\n");
 else if (strcmp(op, "<") == 0)
 printf("Less Than Operator\n");
 else if (strcmp(op, ">") == 0)
 printf("Greater Than Operator\n");
 else if (strcmp(op, "<=") == 0)
 printf("Less Than or Equal To Operator\n");
 else if (strcmp(op, ">=") == 0)
 printf("Greater Than or Equal To Operator\n");
 else if (strcmp(op, "&&") == 0)
 printf("Logical AND Operator\n");
 else if (strcmp(op, "||") == 0)
 printf("Logical OR Operator\n");
 else if (strcmp(op, "!") == 0)
 printf("Logical NOT Operator\n");
 else
 printf("Not an operator\n");
}
int main() {
 char input[5];
 printf("Enter an operator: ");
 scanf("%s", input);
 identify_operator(input);
 return 0;
}
